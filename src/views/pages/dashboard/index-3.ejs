       <div class="col-lg-12">
         <div class="card">
           <div class="card-header">
             <h4 class="card-title">New Student List </h4>
           </div>
           <div class="card-body pt-2">
             <div class="table-responsive">
               <table class="table table-sm mb-0 table-striped text-nowrap">
                 <thead>
                   <tr>
                     <th class="py-3">Student Name</th>
                     <th class="py-3">Assigned Professor</th>
                     <th class="py-3">Branch</th>
                     <th class="py-3">Status</th>
                     <th class="py-3">Date Of Admit</th>
                     <th class="py-3">Edit</th>
                   </tr>
                 </thead>
                 <tbody id="customers">
                  <% if(data && Array.isArray(data)){ %>
                    <% data.forEach((val)=>{ %>
                    <tr class="btn-reveal-trigger">
                      <td class="p-3">
                        <a href="javascript:void(0);">
                          <div class="media d-flex align-items-center">
                            <div class="avatar avatar-xl me-2">
                              <img class="rounded-circle img-fluid" src="<%= val.image %>" width="30" alt="">
                            </div>
                            <div class="media-body">
                              <h5 class="mb-0 fs--1"><%= val.name %></h5>
                            </div>
                          </div>
                        </a>
                      </td>
                      <td class="py-2"><%= val.assigned %></td>
                      <td class="py-2"><%= val.branch %></td>
                      <td>
                        <% if(val.status == 'Pending'){ %>
                          <span class="badge badge-rounded badge-warning"><%= val.status %></span>
                        <% } else if(val.status == 'Suspended'){ %>
                          <span class="badge badge-rounded badge-danger"><%= val.status %></span>
                        <% } else { %>
                          <span class="badge badge-rounded badge-primary"><%= val.status %></span>
                        <% } %>
                      </td>
                      <td class="py-2"><%= val.date %></td>
                      <td>
                        <a href="edit-student" class="btn btn-xs sharp btn-primary">
                          <i class="fa fa-pencil"></i>
                        </a>
                        <a href="javascript:void(0);" class="btn btn-xs sharp btn-danger">
                          <i class="fa fa-trash"></i>
                        </a>
                      </td>
                    </tr>
                    <%  }) 
                  } %>
                 </tbody>
               </table>
             </div>
           </div>
